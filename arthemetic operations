#include<stdio.h>
#include<conio.h>
int main()
{
int a=22,b=14;
int sum=a+b,diff=a-b,mul=a*b,div=a/b,mod=a%b;
printf("the addition is %d",sum);
printf("the subtraction is %d",diff);
printf("the multiplication is %d",mul);
printf("the divison is %d",div);
printf("the percentage is %d",mod);
getch();
return 0;
}
