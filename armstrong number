#include<stdio.h>
#include<conio.h>
void main()
{
int num=153,originalnum,remainder,result=0;
originalnum =num;
while (originalnum!=0)
{
remainder=originalnum %10;
result +=remainder*remainder*remainder;
originalnum /=10;
}
if(result==num)
{
printf("%d is armstrong number",num);
}
else
{
printf("%d is not an armstrong number",num);
}
getch();
}
